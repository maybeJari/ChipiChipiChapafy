{
  "manifest_version": 3,
  "name": "Youtube ChipiChapafy",
  "version": "1.0.0",
  "description": "A funny extension to ChipiChapafy your Youtube!",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "css": ["chipichapa.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Youtube ChipiChapafy"
  }
}
